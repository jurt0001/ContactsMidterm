{
  "name": "contacts",
  "version": "1.0.0",
  "description": "Allows user to see list of contacts and details page of each contact",
  "main": "index.js",
  "scripts": {
    "test": "yes",
    "watch": "webpack --watch",
    "build": "webpack",
    "start": "webpack-dev-server --open",
    "eslint": "^4.8.0",
    "eslint-loader": "^1.9.0"
  },
  "repository": {
    "type": "git",
    "url": "n"
  },
  "keywords": [
    "webpack"
  ],
    "eslintConfig":{
      
           "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        },
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": [
            "error",
            "tab"
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "react/jsx-uses-react": "error", 
        "react/jsx-uses-vars": "error"
    },
        
    "globals": {
            "React": true,
            "ReactDOM": true,
            "Component": true,
            "PropTypes": true,
            "ContactsPage": true,
            "ContactDetailsPage": true,
            "Header": true,
            "Footer": true
        
        }
        
    },
    
  "author": "Christian Jurt",
  "license": "ISC",
  "dependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "commonjs": "0.0.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.8.0",
    "eslint-loader": "^1.9.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "sass-loader": "^6.0.6",
    "webpack": "^3.7.1",
    "webpack-dev-server": "^2.9.1"
  },
  "devDependencies": {
    "eslint": "^4.8.0",
    "eslint-plugin-react": "^7.4.0"
  }
}
